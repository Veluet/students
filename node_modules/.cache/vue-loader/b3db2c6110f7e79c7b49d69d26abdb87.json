grid grid-cols-3 gap-4 \">\n    <div v-for=\"item in items\" :key=\"item.id\" class=\"shadow-sm\">\n      <h2>{{ item.title }}</h2>\n      <p>{{ item.desc }}</p>\n    </div>\n  </div>\n  <div  class=\"grid grid-flow-col grid-cols-3 grid-rows-3 gap-4\">\n    <div v-for=\"year in years\" :key=\"year\">\n      <router-link :to=\"{ name: 'data-summary', params: { year } }\">{{\n        year\n      }}</router-link>\n    </div>\n  </div>\n  <router-view name=\"data-summary\" />\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"schoolYears\",\n  data() {\n    return {\n      items: [\n        {\n          id: 1,\n          title: \"One\",\n          desc: \"See One Now\",\n        },\n        {\n          id: 2,\n          title: \"Two\",\n          desc: \"See Two Now\",\n        },\n        {\n          id: 3,\n          title: \"Three\",\n          desc: \"See Three Now\",\n        },\n      ],\n      students: [],\n      years: []\n    };\n  },\n  created() {\n    axios.get(\"http://apitest.sertifi.net/api/Students\").then((students) => {\n      this.students = students.data;\n\n      // Get all the years from the API request.\n      const getAllYears = students.data.reduce(\n        (arr, item) => arr.concat(item.StartYear, item.EndYear).sort(),\n        []\n      );\n\n      // Now filter out non-unique values and create a years array.\n      this.years = [...new Set(getAllYears)];\n    });\n  },\n};\n</script>\n"]}]}